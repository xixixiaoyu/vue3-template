{
  "name": "vue3-supabase-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,vue,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,vue,json,md,yml,yaml}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:install": "playwright install",
    "db:init": "node scripts/init-database.js",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "build:analyze": "vue-tsc && vite build && npx vite-bundle-analyzer dist/stats.html",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@radix-icons/vue": "^1.0.0",
    "@sentry/vue": "^10.27.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/vue-virtual": "^3.13.12",
    "@unhead/vue": "^2.0.19",
    "@vee-validate/zod": "^4.15.1",
    "@vue/devtools": "^8.0.5",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.294.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.7.1",
    "reka-ui": "^2.6.0",
    "unhead": "^2.0.19",
    "vee-validate": "^4.15.1",
    "vue": "^3.3.9",
    "vue-echarts": "^8.0.1",
    "vue-i18n": "^9.14.5",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.5",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/ui": "^4.0.14",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.7.0",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "radix-vue": "^1.9.17",
    "rollup-plugin-visualizer": "^6.0.5",
    "shadcn-vue": "^2.4.0",
    "supabase": "^2.63.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.14",
    "vue-tsc": "^1.8.19"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
