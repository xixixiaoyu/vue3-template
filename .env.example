# Supabase 配置
# 获取这些值：登录 Supabase Dashboard -> Project Settings -> API
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# 应用基础 URL
BASE_URL=/

# 应用配置
VITE_APP_NAME=Vue 3 Supabase 模板
VITE_APP_DESCRIPTION=基于 Vue 3 和 Supabase 的现代化 SaaS 产品起步模板
VITE_APP_VERSION=1.0.0

# 开发配置
VITE_DEV_MODE=true
VITE_DEBUG_MODE=true

# 功能开关
VITE_ENABLE_OAUTH=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_ANALYTICS=false

# 文件上传配置
VITE_MAX_FILE_SIZE=104857600
VITE_ALLOWED_FILE_TYPES=image/*,application/pdf,text/*

# 实时配置
VITE_REALTIME_ENDPOINT=realtime.supabase.co
VITE_REALTIME_RECONNECT_INTERVAL=5000

# 缓存配置
VITE_CACHE_TTL=3600
VITE_ENABLE_SERVICE_WORKER=false

# 安全配置
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=false

# API 配置
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# 分页配置
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# 主题配置
VITE_DEFAULT_THEME=light
VITE_DEFAULT_LANGUAGE=zh-CN

# 通知配置
VITE_ENABLE_NOTIFICATIONS=true
VITE_NOTIFICATION_DURATION=5000

# 错误报告配置
VITE_ENABLE_ERROR_REPORTING=false
VITE_ERROR_REPORTING_ENDPOINT=

# 分析配置
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=

# 性能监控配置
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# 实验性功能
VITE_ENABLE_EXPERIMENTAL_FEATURES=false
VITE_EXPERIMENTAL_FEATURES=